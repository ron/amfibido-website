{
  "name": "amfibido-website",
  "version": "1.0.0",
  "description": "Website for Amfibido game",
  "main": ".eleventy.js",
  "scripts": {
    "build:css": "postcss css/style.css -o _site/css/style.css -c tailwind.config.js",
    "build:css:prod": "ELEVENTY_ENV=production postcss css/style.css -o _site/css/style.css -c tailwind.config.js",
    "watch:css": "postcss css/style.css -o _site/css/style.css --watch -c tailwind.config.js",
    "start:eleventy": "eleventy --serve",
    "start": "npm-run-all build:css --parallel start:eleventy watch:css",
    "build:eleventy": "eleventy",
    "build:eleventy:prod": "ELEVENTY_ENV=production eleventy",
    "build": "npm-run-all build:css build:eleventy",
    "build:prod": "npm-run-all build:css:prod build:eleventy:prod"
  },
  "keywords": [
    "eleventy",
    "static-site",
    "amfibido"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "latest",
    "autoprefixer": "^10.4.21",
    "luxon": "^3.4.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.0.0"
  }
}
